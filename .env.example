# Hatchery configuration
# Copy to .env and fill in your values.

# GitHub App credentials (required for hatchery-creds)
HATCHERY_GITHUB_APP_ID=
HATCHERY_GITHUB_APP_KEY=/path/to/private-key.pem
HATCHERY_GITHUB_INSTALLATION_ID=

# Headscale/Tailscale (required for spawn)
HATCHERY_HEADSCALE_AUTH_KEY=
HATCHERY_TAILSCALE_DOMAIN=tail.levinkeller.de

# Socket directory for credential service
HATCHERY_SOCKET_DIR=/var/run/hatchery
